         <xs:enumeration value="ArtistVocalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for the process of recording vocals on a project and dealing with technical issues and decisions. He or she operates the recording equipment (or supervises the operation of the recording equipment) during vocal Performances and makes aesthetic choices in order to obtain the sound and concepts the artist and producer are looking for. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ArtistVocalSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an ArtistVocalEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantCameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assitant of a CameraOperator.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantChiefLightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the second head of the LightingTechnicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a Director.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is delegated some area of responsibility on a project, but reports to the Producer or CoProducer(s).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssistantVisualEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assitant of a VisualEditor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AuralTrainer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who trains Performers in the development of aural skills.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Binder">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who binds an item.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BindingDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a book binding.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a book (including arrangement of type and illustration, choice of materials, etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookjackDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a dust jacket for a book.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookplateDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a bookplate (an identification label commonly on the inside front cover of books).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BookProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a book.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BroadcastAssistant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party providing general support in the production of a radio programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BroadcastJournalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party working as a Journalist for a broadcast programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating a camera.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Carpenter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who carries out woodwork.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CastingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party selecting talent (actors, singers, dancers, etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Censor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Editor who examines Creations and removes objectionable content.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChiefLightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the head of the LightingTechnicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Choreographer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a dance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClapperLoader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who operates the clapboard at the beginning of a shot.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoExecutiveProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An ExecutiveProducer who is not the only ExecutiveProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CommissioningBroadcaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party commissioning material for a broadcast programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CompilationProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for supervising and/or exercising control over the work assigned to other Producers, Engineers, and others on the project.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Consultant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party giving professional advice on matters of production technique or on content who does not appear in the programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ContinuityChecker">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who checks the continuity of a series of camera shots.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Contractor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is hired to perform work.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer who is sharing some or all of a producer's responsabilities with a Producer or another CoProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Correspondent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the writer of a letter or other communication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CostumeDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a costume.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CoverDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of a cover.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Designer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator of a design.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DialogueCoach">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who helps train an actor in diction and/or the use of dialects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalAudioWorkstationEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer is responsible for the operation of a Digital Audio Workstation (DAW) during recording and mix sessions including the archiving and maintenance of the recorded material (basically data files). The DigitalAudioWorkstationEngineer is not generally called upon to make major aesthetic judgments.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer responsible for operating equipment for editing (in today’s world generally a digital audio workstation). They can, from time to time, be called on to make aesthetic choices.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DigitalEditingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a DigitalEditingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Director">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who leads or supervises actors, e.g. in the prodution of a movie.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DirectStreamDigitalEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the functioning and maintenance of digital recordings using Direct Stream Digital format equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DistributionCompany">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Organization responsible for delivering material provided by a ProductionCompany.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who helps an actor to put on and take off a costume.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dubber">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator who adds the dubbing into a Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Editor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the preparation of a publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditorInChief">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The head of editorial staff.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="EditorOfTheDay">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the preparation of a news broadcast of the day.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Encoder">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Creator processing and encoding a Resource for distribution.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Engineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for technical aspects. This includes RecordingEngineer, but may also cover roles in other content production processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ExecutiveProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who secures funding for a project. He or she may be responsible for the overall concept. He or she may monitor the project from time to time to ensure that the producer and Artist are on track.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Expert">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who contributes his expertise to a programme.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FightDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for the choreography of fight scenes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director of a movie.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmDistributor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Publisher of films.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who edits an audio-visual production for publishing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party (individual or company) that is responsible for the financing of an audio-visual production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FilmSoundEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that is responsible for (part of) the soundtrack used in an audio-visual Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FloorManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party acting on the studio floor on behalf of the Director.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FocusPuller">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A CameraOperator who adjusts the focus of the camera.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates incidental sound effects in post-production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyEditor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who edits sound effects created by a FoleyArtist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FoleyMixer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A SoundMixer who works with a FoleyArtist to create sound effects.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicAssistant">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party drawing pictures or picture elements as content for programmes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GraphicDesigner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Designer of graphical elements.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Greensman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party procuring and maintaining vegetation on a set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Grip">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supporting CameraOperators and LightingTechnicians with specialized equipment (e.g. camera cranes).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Hairdresser">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who prepares the hair of an Actor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InitialProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party that initiates the creation of a SoundRecording or Resource and is sometimes referred to as a commissioning rights holder. An InitialProducer may be a Person or an Organization and the term contrasts with the role of a StudioProducer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyGrip">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is the head of the Grips.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Leadman">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is responsible for the crew in charge of the set decoration.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LightingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the lighting.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LightingTechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating lighting equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LocationManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party managing all aspects relating to filming in specific places.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MakeUpArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who prepares the make up of an Actor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Manufacturer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for manufacturing a device or a piece of technical equipment.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MasteringEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer who is the last creative bridge between the mix process and the distribution process. He or she generally works with a final mix of a project and makes final adjustments to the sound before distribution while maintaining the vision of the artist, producer and label.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MasteringSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MasteringEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MatteArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who creates artworks used in making scenery and locations that do not exist.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer combining different audio recording elements into a final version. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a MixingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for music in the production of a film or a radio program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NewsProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a news program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OverdubEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer combining different audio recordings into an overdub.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OverdubSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an OverdubEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PhotographyDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Director responsible for photography.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PostProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for post-production processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProgrammingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the functioning and maintenance of digital recorders and digital audio workstations. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PreProduction">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer responsible for pre-production processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PreProductionEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Engineer involved in pre-production processes.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionCompany">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Organization responsible for a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionDepartment">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A department of a ProductionCompany responsible for a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Person who coordinates a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProductionSecretary">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party providing secretarial support in a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ProgramProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a program, e.g. a TV or radio program. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PropertyManager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for purchasing, acquiring and/or manufacturing anything needed for a production.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PublishingDirector">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for the coherence and continuity of a publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Pyrotechnician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party operating equipment to produce fire and explosions.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RecordingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party supervising the technical aspects of a sound or video recording Session.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RecordingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a RecordingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Redactor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Editor who prepares text for publication.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ReissueProducer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A producer of a reissue of a SoundRecording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RemixingEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A MixingEngineer who creates a finished product that is different from the original master album or single mix, e.g. a Dance Remix, Radio Remix, Rock Remix.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RemixingSecondEngineer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of a RemixingEngineer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Repetiteur">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who coaches ballet Dancers or opera singers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Researcher">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for researching a project (e.g. gathering ideas, background information etc.).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ResearchTeamHead">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The head of a team of Researchers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ResearchTeamMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of a team of Researchers.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Restager">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party responsible for restaging a Work (with minimal contribution to the content).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Rigger">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Grip operating equipment to satisfy rigging needs on a set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="RightsControllerOnProduct">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A RightsController as it appears on a product.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Runner">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is a junior member of the production team doing all kinds of supporting jobs.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScenicOperative">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party in charge of a studio set.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScientificAdvisor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party providing scientific advice.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ScriptSupervisor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who tracks the progress of a production based on a script.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SecondAssistantCameraOperator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An assistant of an AssistantCameraOperator.</xs:documentation>
