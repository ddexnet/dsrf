   <xs:simpleType name="RequestReason">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of Reason for requesting something.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="DisputeResolutionRequest">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Event in which aid in resolving a Dispute is requested.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GeneralRequest">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Event in which in which a work/share information request is a 'broadcast' exercise (i.e. where the requester does’t have good ownership information to make a SpecificRequest).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SpecificRequest">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Event in which a work/share information request is for a 'targeted' exercise where the requester already has (or believes to have) good information about the owner.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefined">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity which is defined by a sender of a DdexMessage in a manner acceptable to its recipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ResourceContributorRole">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Role played by a Contributor in relation to a Fixation of an abstract Creation.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Accompanyist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who plays a musical Instrument while another person is singing (or playing another instrument).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Actor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs spoken word or mime.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AdditionalPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer that is not a LeadPerformer.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AnchorPerson">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party acting as the central moderator of a program.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Announcer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party reading news or describing events.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Artist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A principal Contributor to a Performance of a MusicalWork or a NonMusicalWork that results in the creation of a Resource, or a principal creative Contributor to a Resource that is not a result of a Performance. Note: Used for naming groups as well as individuals.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="AssociatedPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist commonly associated with a Work as one of its Performers, and whose identity is only used for accurate Work identification.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BackgroundVocalist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who sings in the background.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="BandLeader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The principal member of a MusicGroup.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Causeur">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who gives an informal lecture, typically on radio.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Choir">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of Parties who perform vocally together. Typically, Choirs consist of at least 2 people in an combination of different vocal ranges.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChoirMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of a Choir.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ChorusMaster">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Conductor who directs vocalists in a Choir.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="CircusArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist with skills that ar typical for circus performances.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ClubDJ">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who announces and plays recorded music.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Comedian">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Artist who makes people laugh (by telling funny stories or by acting).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Commentator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who provides interpretation, analysis, or a discussion of a subject matter.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Conductor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who leads or conducts a Performance by a group of Musicians.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Dancer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a dance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Ensemble">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of two or more Parties performing a MusicalWork together. Note: An Ensemble may be of any size or any grouping of Performers from a vocal duo to a full orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FeaturedArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is not the MainArtist but is acknowledged as a significant Contributor to the Performance. Note: FeaturedArtists are often MainArtists on their own Resources. They are also frequently credited on marketing material using the term 'featuring ...'.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="GroupMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Person that is a member of a MusicGroup.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="InterviewedGuest">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is interviewed.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Interviewer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who interviews, i.e. talks to somebody for the purpose of eliciting his opinions.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyCharacter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A central or primary Character in a storyline.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="KeyTalent">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party performing as a leading character.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LeadPerformer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer that plays the role of the protagonist in a film or play.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MainArtist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who is a principal credited Artist for a Resource. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MusicGroup">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A group of individuals who perform vocally and/or instrumentally together.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Musician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either vocally or instrumentally.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Narrator">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who tells a story or gives an account of an event.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NewsReader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who reads news stories.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Orchestra">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A large group of Parties performing a MusicalWork together, predominantly using musical instruments rather than voice. An Orchestra is typically led by a Conductor.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OrchestraMember">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A member of an Orchestra.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="PrimaryMusician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either vocally or instrumentally and would be considered the principal Contributor for the piece.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Puppeteer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who operates puppets in a Performance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SignLanguageInterpreter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party translating spoken words into sign language.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Soloist">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs the featured Part of a MusicalWork (or a section of it) alone or with only supporting accompaniment. </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Speaker">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs using his voice without singing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StoryTeller">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Performer relaying a story with dramatic or theatrical interpretation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="StudioMusician">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs a MusicalWork either vocally or instrumentally in a studio.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Stunts">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party performing stunts.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SupportingActor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An Actor playing a secondary role.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Unknown">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity used when a sender of a DdexMessage wishes to indicate that the value within the allowed value set is unknown.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefined">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Type of an Entity which is defined by a sender of a DdexMessage in a manner acceptable to its recipient.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VoiceActor">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Party who performs using his voice.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ResourceGroupType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of ResourceGroup.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Carrier">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A ResourceGroupType indicating that a Release is made up of several carriers, e.g. of two CDs.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Side">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A ResourceGroupType indicating that a Release is made up of several Sides.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MultiWorkPart">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A ResourceGroupType indicating that a Release is made up of a compound Work. This is often used to separate classical movements.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ResourceRelationshipType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of relationship between two Resources.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="ddex:HasContentFrom">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A ResourceRelationshipType of a Resource that is closely related to another Resource because some content (e.g. a song) has been taken from it.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="IsPartOf">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A ResourceRelationshipType of a Resource that is contained in the primary Resource.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ResourceType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of Resource.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Image">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A still visual Recording or illustration of an object. Examples: a Photograph or a GraphicImage.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="MIDI">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Set of computer instructions which enables a sound processor to generate sounds (using the Musical Instrument Digital Interface as defined by the MIDI Manufacturers Association (MMA)).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SheetMusic">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A hand-written or printed form of musical notation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Software">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A program that enables a Computer to perform a specific task(s).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundRecording">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An audio Recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Text">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A written Manifestation. Examples: the Lyrics of a MusicalWork or the text of an interview.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UserDefinedResource">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Resource which is UserDefined.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Video">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An audio-visual Recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="ResourceType_MCNOTIF">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of Resource.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="SheetMusic">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A hand-written or printed form of musical notation.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SoundRecording">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An audio Recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Video">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">An audio-visual Recording.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="RevenueSourceType">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of Revenue earned by the SoundRecording, according to the way the Revenue is generated.</xs:documentation>
      </xs:annotation>
