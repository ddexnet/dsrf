   <xs:simpleType name="RatingReason">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Reason for a rating.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Behaviour">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Potentially dangerous behaviour which young children are likely to copy.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Blasphemy">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Blasphemy.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Crime">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">[missing definition]</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="DiscriminationOrPrejudice">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Discriminatory language or behaviour.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Drugs">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Encouragement of the use of tobacco or alcohol or depictions of the use of illegal drugs.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ExplicitSex">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Explicit sex.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="ExtremeViolence">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Extreme Violence.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="FearOrHorror">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Pictures or sounds likely to be scary or frightening to young children.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Gambling">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Encouragement of the use of games of chance.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="IllegalDrugs">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Illegal drugs.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Language">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Swearing, offensive language, sexual expletives or blasphemy.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="LegalDrugs">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Legal drugs.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Nudity">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Nudity.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="OnlineGameplay">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">[missing definition]</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Sex">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Obvious sexual innuendo or explicit sexual descriptions or images or sexual posturing; erotic or sexual nudity; sexual intercourse.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SexualViolence">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Sexual violence.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Theme">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Problematic themes, e.g. crime, suicide.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Violence">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Violence.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
