            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="CommercialModelType_MWNL">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Type of CommercialModel (e.g. SubscriptionModel and PayAsYouGoModel), as used in the MWNL messages. The CommercialModelType indicates how a Consumer pays for a Service or Release.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="AdvertisementSupportedModel">
