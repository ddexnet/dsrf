               <xs:documentation source="ddex:Definition">The Identifier (specific to the Message) of the Summary Record. This Identifier shall be used by Usage/Revenue/Sales Records to indicate to which Summary Record they provide details.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DistributionChannel" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Licensee on whose behalf the Sales/Usage has been generated.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DistributionChannelDPID" minOccurs="0">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The DDEX Party ID of the DistributionChannel.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="CommercialModel" type="avs:CommercialModelType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of CommercialModel of the SalesTransaction. The CommercialModel indicates how a Consumer pays for a Service or Release.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="UseType" type="avs:UseType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The nature of a Service, or a Release, as used by a Consumer.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Territory" type="avs:CurrentTerritoryCode">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Territory for which the sale/transaction is reported.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ServiceDescription" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The service name (e.g. a service tier) under which the SalesTransaction took place. The ServiceDescription may not contain space characters or underscores.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Usages" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of Usages of the Release from the MessageSender to Consumers, including any sales adjustments. Sales adjustments include e.g. file failures. For the avoidance of doubt: this cell shall contain all relevant Usages of Releases plus all relevant Usages of Resources.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Subscribers" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of paying Subscribers.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Currency" type="avs:CurrencyCode">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Currency in which the NetRevenue, ConsumerPaidUnitPrice, AllocatedRevenue and AllocatedNetRevenue are provided (represented by an ISO 4217 CurrencyCode).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NetRevenue" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Net Revenue of the MessageSender for the Territory, CommercialModel and UseType. This element is, in most cases, the basis for royalty calculations.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="RightsController" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Name of the RightsController that is believed by the MessageSender to control a RightShare of the Work.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="RightsControllerPartyId" minOccurs="0">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">PartyId of the RightsController that is believed by the MessageSender to control a RightShare of the Work.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value=".*::.*"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="AllocatedUsages" minOccurs="0" maxOccurs="unbounded" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of Usages allocated to the RightsController.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AllocatedRevenue" minOccurs="0" maxOccurs="unbounded" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The amount due to the RightsController for the Usages, Revenues or Sales reported.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AllocatedNetRevenue" minOccurs="0" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Net Revenue of that which is generated that is to be ascribed to the RightsController.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="RightsControllerMarketShare" minOccurs="0" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The ratio in percent between number of Usages and
number of AllocatedUsages. </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ConsumerPaidUnitPrice" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The average price paid by the Subscribers.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FreeOrTrialSubscribers" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of non-paying Subscribers (including Subscribers in a non-paying trial period).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ExchangeRateBaseCurrency" minOccurs="0" type="avs:CurrencyCode">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The base Currency for which the ExchangeRate is given.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ExchangeRate" minOccurs="0" type="xs:decimal">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The exchange rate used to calculate the AllocatedRevenue in the specified Currency.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="ResourceIdentificationGroupingForUgcProfile">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing a grouping for Resource identification in the UgcProfile.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:choice maxOccurs="unbounded">
            <xs:sequence>
               <xs:element name="RecordType-AS01" type="dsrf:RecordType-AS01">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing a Record, of type AS01, which contains information regarding the SoundRecordings,
Videos and other 'master' Resources that have been used in creating the user-generated content that has been accessed. </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="RecordType-MW01" minOccurs="0" maxOccurs="unbounded"
                  type="dsrf:RecordType-MW01">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing a Musical Work Record,
of type MW01.01 which contains information about the MusicalWork utilised. </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:element name="RecordType-AS02" type="dsrf:RecordType-AS02">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a Resource Record of type AS02.01 describing the Resource including basic information of the underlying Work.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="UgcProfile">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a Report for all Usage, Revenue or Sales for user-generated content.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RecordType-HEAD" type="dsrf:RecordType-HEAD">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The MessageHeader for the SalesReportMessage.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice maxOccurs="unbounded">
            <xs:element name="RecordType-SY02" type="dsrf:RecordType-SY02">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a type of use, commercial model and territory any Release contained in the Sales Report Message has been traded under.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="RecordType-SY03" type="dsrf:RecordType-SY03">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a type of use, commercial model and territory any Release contained in the Sales Report Message has been traded under.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="UgcProfileBlock" minOccurs="0" maxOccurs="unbounded"
            type="dsrf:UgcProfileBlock">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a block in the UgcProfile.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="RecordType-FOOT" type="dsrf:RecordType-FOOT">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The MessageFooter for the SalesReportMessage.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="UgcProfileBlock">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a block in the UgcProfile.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ResourceIdentificationGrouping" maxOccurs="unbounded"
            type="dsrf:ResourceIdentificationGroupingForUgcProfile">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a grouping for Resource identification.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="RecordType-RU01" type="dsrf:RecordType-RU01">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a UGC Release Usage Record of type RU01 describing the use of UGC Releases.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="RecordType-RU02" type="dsrf:RecordType-RU02">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a RU02 Record for each UGC Release and its use.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:sequence minOccurs="0" maxOccurs="unbounded">
            <xs:element name="SU03" type="dsrf:RecordType-SU03">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a Record providing Usage, Revenue or Sales Figures of type SU03.01.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:sequence minOccurs="0" maxOccurs="unbounded">
               <xs:element name="RecordType-LI01" type="dsrf:RecordType-LI01">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing a LI01 Record providing Licensor-specific information for the usages reported in the preceding SU03.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:sequence>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="ddex_IsoDate">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Date represented in ISO 8601:2004 format: YYYY[-MM[-DD]].</xs:documentation>
         <xs:documentation source="ddex:Comment">Format: A Date represented as a calendar Year, Month or Day (in ISO 8601:2004 format: YYYY, YYYY-MM or YYYY-MM-DD).</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:pattern value="[0-9]{4}(-[0-9]{2}){0,1}(-[0-9]{2}){0,1}"/>
      </xs:restriction>
   </xs:simpleType>
</xs:schema>
