<xs:schema xmlns:dsrf="http://ddex.net/xml/dsrf/30"
           xmlns:avs="http://ddex.net/xml/avs/avs"
           xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://ddex.net/xml/dsrf/30"
           elementFormDefault="unqualified"
           attributeFormDefault="unqualified">
   <xs:import namespace="http://ddex.net/xml/avs/avs"
              schemaLocation="avs.xsd"/>
   <xs:annotation>
      <xs:documentation>© 2006-2015 Digital Data Exchange, LLC (DDEX)</xs:documentation>
      <xs:documentation>This XML Schema Definition file is, together with all DDEX standards, subject to two licences: If you wish to evaluate whether the standard meets your needs please have a look at the Evaluation Licence at https://kb.ddex.net/display/HBK/Evaluation+Licence+for+DDEX+Standards. If you want to implement and use this DDEX standard, please take out an Implementation Licence. For details please go to http://ddex.net/apply-ddex-implementation-licence.</xs:documentation>
   </xs:annotation>
   <xs:element name="SalesReportMessage">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A SalesReportMessage in the DSR Message Suite, Flat File Profile, sent to a MusicalWork Licensor.</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:choice>
            <xs:element name="SingleFileSalesReportMessage"
                        type="dsrf:SingleFileSalesReportMessage">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A SingleFileSalesReportMessage in the DSR Message Suite, Flat File Profile, sent to a record company or a MusicalWork Licensor.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="MultiFileSalesReportMessage"
                        type="dsrf:MultiFileSalesReportMessage">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A MultiFileSalesReportMessage in the DSR Message Suite, Flat File Profile, sent to a record company or a MusicalWork Licensor.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="AcknowledgementMessage">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A sales reporting Message in the DSR Message Suite, Flat File Profile, sent to a MusicalWork Licensor.</xs:documentation>
      </xs:annotation>
      <xs:complexType/>
   </xs:element>
   <xs:complexType name="AudioVisualReleaseBlock">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of Releases, Resources, Musical Works and their Usage, Revenue or Sales figures.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ReleaseRecord03" type="dsrf:ReleaseRecord03">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a Release Record of type RE03 describing an audio-visual Head Release.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ResourceRecord03"
                     maxOccurs="unbounded"
                     type="dsrf:ResourceRecord03">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a Resource Record describing Resources of type AS03. These Records provide information regarding all the Videos and other Resources that are contained in the Head Release.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CueRecord"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dsrf:CueRecord">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a Cue Record of type CU01. Cue Records allow the identification and description of cues, i.e. information about Musical Works or Resources that have been used in a specific Resource and how they have been used.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReleaseRecord02"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dsrf:ReleaseRecord02">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a Record describing 'Sub-Releases' of type RE02. These Sub-Releases are either single-Resource Track Releases (as communicated in a NewReleaseMessage) and/or Releases that the Message Sender has generated exclusively from the Resources that are used in the Head Release.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="UsageRevenueSalesRecord04"
                     maxOccurs="unbounded"
                     type="dsrf:UsageRevenueSalesRecord04">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing a Record providing Usage, Revenue or Sales figures of type SU04.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="AudioVisualReleaseProfile">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a report for all audio-visual Usage, Revenue or Sales.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="SummaryRecord04"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dsrf:SummaryRecord04">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a type of use, commercial model and territory any Release contained in the Sales Report
Message has been traded under. </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="Block"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dsrf:AudioVisualReleaseBlock">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of Releases, Resources, Musical Works and their Usage, Revenue or Sales figures.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="BasicAudioReleaseProfile">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of a report for audio-only Usage, Revenue or Sales. This Profile is also for reporting Usage, Revenue or Sales for music videos.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="SummaryRecord01"
                     maxOccurs="unbounded"
                     type="dsrf:SummaryRecord01">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Composite containing details of a type of use, commercial model and territory any Release contained in the Sales Report
Message has been traded under. </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:sequence maxOccurs="unbounded">
            <xs:element name="ReleaseRecord01" type="dsrf:ReleaseRecord01">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a Release Record of type RE01 describing a Head Release.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:choice maxOccurs="unbounded">
               <xs:sequence>
                  <xs:element name="ResourceRecord01" type="dsrf:ResourceRecord01">
                     <xs:annotation>
                        <xs:documentation source="ddex:Definition">A Composite containing a Resource Record describing Resources of type AS01. These Records provide information regarding all the Sound Recordings, Videos and other Resources that are contained in the Head Release.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="MusicalWorkRecord"
                              minOccurs="0"
                              maxOccurs="unbounded"
                              type="dsrf:MusicalWorkRecord">
                     <xs:annotation>
                        <xs:documentation source="ddex:Definition">A Composite containing a Musical Work Record of type MW01 describing Musical Works. These Records provide information regarding all the Musical Works that are utilised in the Head Release and/or one of its Resources.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
               <xs:element name="ResourceRecord02" type="dsrf:ResourceRecord02">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing a Resource Record describing Resources of type AS02. These Records provide information regarding all the Sound Recordings, Videos and other Resources that are contained in the Head Release.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:choice>
            <xs:element name="ReleaseRecord02"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        type="dsrf:ReleaseRecord02">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing a Record describing 'Sub-Releases' of type RE02. These Sub-Releases are either single-Resource Track Releases (as communicated in a NewReleaseMessage) and/or Releases that the Message Sender has generated exclusively from the Resources and/or Musical Works that are used in the Head Release.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:choice maxOccurs="unbounded">
               <xs:element name="UsageRevenueSalesRecord01" type="dsrf:UsageRevenueSalesRecord01">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing a Record providing Usage, Revenue or Sales figures of type SU01.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="UsageRevenueSalesRecord02" type="dsrf:UsageRevenueSalesRecord02">
                  <xs:annotation>
                     <xs:documentation source="ddex:Definition">A Composite containing a Record providing Usage, Revenue or Sales figures of type SU02.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:choice>
         </xs:sequence>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="CueRecord">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">Basic Work Cue Record.</xs:documentation>
         <xs:documentation source="ddex:Definition">A Composite containing a Basic Work Cue Record with details of Cues of Works used in video Resources that are part of an audio-visual Release.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RecordType" type="avs:RecordType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of the Record; always to contain 'CU01'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="BlockId" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier (specific to the Message) of the Block that this Record belongs to. All Records in a single Block shall have the same BlockId.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CueResourceReference" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier (specific to the Block) of the Record, that allows other Records to reference this Record.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CueStartTime" minOccurs="0" type="xs:duration">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The start time of the referenced Creation, measured from the start of the Resource (using the ISO 8601:2004 PT[[hhH]mmM]ssS format.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CueDuration" type="xs:duration">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Duration of the use of the referenced Creation (using the ISO 8601:2004 PT[[hhH]mmM]ssS format.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedCreationISRC" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The ISRC of the referenced Creation.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedCreationISWC" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The ISWC of the referenced Creation.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedCreationTitle" minOccurs="0" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Title of the referenced Creation.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedCreationDisplayArtistName"
                     minOccurs="0"
                     type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name to be used by a DSP when presenting Artist details of the referenced Creation to a Consumer.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedCreationDisplayArtistPartyId" minOccurs="0">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The PartyId of the DisplayArtist.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="ReferencedCreationContributorName"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name of a contributor of the referenced Creation. If it is a Musical Work, this may be the publisher. This Cell can also be used for a creative contributor when the MessageSender does not know the contributor’s role.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedCreationContributorPartyId"
                     minOccurs="0"
                     maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The PartyId(s) of the Contributor(s).</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="ReferencedWorkComposerAuthorName"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name of a Writer of the referenced Musical Work.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedWorkComposerAuthorPartyId"
                     minOccurs="0"
                     maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The PartyId(s) of the ComposorAuthor(s).</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="ReferencedWorkArrangerName"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Name of an Arranger of the referenced Musical Work.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReferencedWorkArrangerPartyId"
                     minOccurs="0"
                     maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The PartyId(s) of the Arranger(s).</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="FileFooter">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">File Footer Record for all Sales Report Message formatted in accordance with this standard if a Sales Report Message is split into several files.</xs:documentation>
         <xs:documentation source="ddex:Definition">A Composite placed at the end of each File if a Sales
Report Message is split into several Files. </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RecordType" type="avs:RecordType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of the Record; always to contain 'FFOO'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NumberOfLines" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Number of data lines communicated in the file, including Header and Footer Records as well as empty Records in accordance with Cl
ause 6.6.7 and commented-out Records in accordance with Clause 6.6.9. </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="FileHeader">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">File Header Record for all Sales Report Message formatted in accordance with this standard if a Sales Report Message is split into several files.</xs:documentation>
         <xs:documentation source="ddex:Definition">A Composite placed at the beginning of each File if a Sales
Report Message is split into several Files. </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RecordType" type="avs:RecordType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of the Record; always to contain 'FHEA'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MessageId" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Identifier of the Sales Report Message. The Message ID should be unique for a MessageSender and never be re-used by the MessageSender.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="FileNumber" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Sequence number of the file within the Flat File Sales Report Message. The first file shall be numbered “1”. As the first file does not contain a FHEA Record, the first FHEA header shall contain a File Number value of “2”.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NumberOfFiles" minOccurs="0" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Number of files of that the Flat File Sales Report Message has been split into. This can be omitted if not known.</xs:documentation>
            </xs:annotation>
         </xs:element>
        <xs:element name="HasCaptioning" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A Flag indicating whether the audio-visual Resource has captioning for the hard of hearing and/or deaf.</xs:documentation>
            </xs:annotation>
         </xs:element>
        <xs:element name="SenderPartyId">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The DDEX PartyID (DPID) of the MessageSender.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:pattern value="PADPIDA[a-zA-Z0-9]+"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="FirstSalesReportMessageFile">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of the first SalesReportMessage File.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="MessageHeader" type="dsrf:MessageHeader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The MessageHeader for the FirstSalesReportMessageFile.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="BasicAudioReleaseProfile" type="dsrf:BasicAudioReleaseProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for audio-only Usage, Revenue or Sales. This Profile is also for reporting Usage, Revenue or Sales for music videos.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ResourceOnlyProfile" type="dsrf:ResourceOnlyProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for Usages, Revenues or Sales for Resources for which no information on the Release it is contained in has been previously made available.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="UGCProfileMasterRecording" type="dsrf:UgcProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for all Usage, Revenue or Sales for user-generated content.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="UGCProfileNoMasterRecording" type="dsrf:UgcProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for all Usage, Revenue or Sales for user-generated content.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="AudioVisualReleaseProfile" type="dsrf:AudioVisualReleaseProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for all audio-visual Usage, Revenue or Sales.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="FileFooter" type="dsrf:FileFooter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The FileFooter for the FirstSalesReportMessageFile.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="LastSalesReportMessageFile">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">A Composite containing details of the last SalesReportMessage File.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="FileHeader" type="dsrf:FileHeader">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The FileHeader for the LastSalesReportMessageFile.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="BasicAudioReleaseProfile" type="dsrf:BasicAudioReleaseProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for audio-only Usage, Revenue or Sales. This Profile is also for reporting Usage, Revenue or Sales for music videos.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="ResourceOnlyProfile" type="dsrf:ResourceOnlyProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for Usages, Revenues or Sales for Resources for which no information on the Release it is contained in has been previously made available.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="UGCProfileMasterRecording" type="dsrf:UgcProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for all Usage, Revenue or Sales for user-generated content.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="UGCProfileNoMasterRecording" type="dsrf:UgcProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for all Usage, Revenue or Sales for user-generated content.</xs:documentation>
               </xs:annotation>
            </xs:element>
            <xs:element name="AudioVisualReleaseProfile" type="dsrf:AudioVisualReleaseProfile">
               <xs:annotation>
                  <xs:documentation source="ddex:Definition">A Composite containing details of a report for all audio-visual Usage, Revenue or Sales.</xs:documentation>
               </xs:annotation>
            </xs:element>
         </xs:choice>
         <xs:element name="MessageFooter" type="dsrf:MessageFooter">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The MessageFooter for the LastSalesReportMessageFile.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="MessageFooter">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">Global Footer Record for all Sales Report Message formatted in accordance with this standard.</xs:documentation>
         <xs:documentation source="ddex:Definition">A Composite placed at the end of each SalesReportMessage.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RecordType" type="avs:RecordType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of the Record; always to contain 'FOOT'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NumberOfLines" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">Number of data lines communicated in the Sales Report Message, including Header and Footer Records as well as empty Records in accordance with Clause 6.6.7 and commented-out Records in accordance with Clause 6.6.9.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="MessageHeader">
      <xs:annotation>
         <xs:documentation source="ddex:Definition">Global Header Record for all Sales Report Message formatted in accordance with this standard.</xs:documentation>
         <xs:documentation source="ddex:Definition">A Composite placed at the beginning of each SalesReportMessage providing information about the Message, such as MessageSender, MessageRecipient and a Message creation time stamp.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="RecordType" type="avs:RecordType">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Type of the Record; always to contain 'HEAD'.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MessageVersion" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Version of the Message.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ProfileVersion" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The Identifier of the Version of the Profile used for the Message. This is the heading of the relevant profile clause within this standard.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MessageId" type="xs:string">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">A string used to uniquely identify the Message. The MessageId shall be, in combination with the MessageSenderId, globally unique. Thus, a MessageSender shall never re-use a MessageId.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MessageCreatedDateTime" type="dsrf:ddex_IsoDate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The DateTime on which the Message was created (the only allowed format is ISO 8601:2004: YYYY-MM-DDThh:mm:ssTZD).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="NumberOfFiles" type="xs:integer">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The number of files that the Sales Report is provided in.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="UsageStartDate" type="dsrf:ddex_IsoDate">
            <xs:annotation>
               <xs:documentation source="ddex:Definition">The StartDate of the reporting Period covered by the Message (in ISO 8601:2004 format). This is a string with the syntax YYYY[-MM[-DD]].</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="UsageEndDate" type="dsrf:ddex_IsoDate">
